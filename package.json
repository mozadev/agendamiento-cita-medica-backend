{
  "name": "agendamiento-cita-medica",
  "version": "1.0.0",
  "description": "Sistema de agendamiento de citas médicas para asegurados - Reto Técnico Backend",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f",
    "offline": "serverless offline"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "typescript",
    "clean-architecture",
    "appointments",
    "healthcare"
  ],
  "author": "Fabrizio Moza",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/client-eventbridge": "^3.654.0",
    "@aws-sdk/client-sns": "^3.654.0",
    "@aws-sdk/client-sqs": "^3.654.0",
    "@aws-sdk/lib-dynamodb": "^3.654.0",
    "mysql2": "^3.11.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^30.2.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "serverless": "^4.4.6",
    "serverless-esbuild": "^1.54.3",
    "serverless-offline": "^14.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
